func round(){
	if ($1>0){
		return int($1+0.5)
	} else {
		return int($1-0.5)
	}
}

objref v1
proc createVector(){
	startVal = $1
	stopVal  = $2
	stepVal  = $3
	numel    = (stopVal-startVal)/stepVal
	v1       = new Vector(numel+1)
	v1.x[0]  = startVal

	for i=1,numel{
		v1.x[i] = v1.x[i-1]+stepVal
	}
}

// Create list of numbers 0:0.1:10 to search from
createVector(0,10,0.1)
